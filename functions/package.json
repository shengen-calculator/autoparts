{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "lint": "eslint .",
    "serve": "firebase serve --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy:main": "firebase deploy --only functions:main",
    "deploy:order": "firebase deploy --only functions:order",
    "deploy:search": "firebase deploy --only functions:search",
    "deploy:dataImport": "firebase deploy --only functions:dataImport",
    "deploy:statistic": "firebase deploy --only functions:statistic",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "@google-cloud/datastore": "^6.1.0",
    "base64-stream": "^1.0.0",
    "exceljs": "^1.15.0",
    "firebase-admin": "^8.13.0",
    "firebase-functions": "^3.7.0",
    "imap": "^0.8.19",
    "mssql": "^6.2.0"
  },
  "devDependencies": {
    "eslint": "^5.12.0",
    "eslint-plugin-promise": "^4.0.1",
    "firebase-functions-test": "^0.1.6"
  },
  "private": true
}
